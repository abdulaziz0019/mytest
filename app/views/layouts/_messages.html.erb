<div class="rows">
  <div class="col-xs-10 col-xs-offset">
    <% flash.each do |name,msg| %>
    <div class='alert alert-<%="#{name}"%> role="alert"'>
      <a herf= "#" class="close" data-dismiss="alert">&#215; </a>
      <%= content_tag :div, msg, :id => "flash #{name}" %>
    </div>
    <% end %>
  </div>
</div>
